{
  "name": "n8n-nodes-onlyoffice",
  "version": "1.0.3",
  "description": "n8n community node for OnlyOffice DocSpace integration - manage files, folders, and documents in your OnlyOffice workspace",
  "keywords": [
    "n8n-community-node-package",
    "n8n",
    "onlyoffice",
    "docspace",
    "document",
    "collaboration",
    "office",
    "workflow",
    "automation"
  ],
  "license": "MIT",
  "homepage": "https://manojnair.online/",
  "author": {
    "name": "Manoj Nair",
    "email": "manoj@manojnair.online",
    "url": "https://manojnair.online/"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ManojNairOnline/OnlyOffice.git"
  },
  "main": "index.js",
  "scripts": {
    "build": "tsc && gulp build:icons && gulp build:structure",
    "dev": "tsc --watch",
    "format": "prettier nodes credentials --write",
    "lint": "eslint *.ts package.json",
    "lintfix": "eslint *.ts package.json --fix",
    "prepublishOnly": "npm run build"
  },
  "files": [
    "dist",
    "credentials",
    "nodes"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [
      "dist/credentials/OnlyOfficeApi.credentials.js"
    ],
    "nodes": [
      "dist/nodes/OnlyOffice/OnlyOffice.node.js"
    ]
  },
  "devDependencies": {
    "@types/node": "^18.16.0",
    "@typescript-eslint/parser": "^5.59.0",
    "eslint": "^8.42.0",
    "eslint-plugin-n8n-nodes-base": "^1.11.0",
    "gulp": "^4.0.2",
    "n8n-workflow": "*",
    "prettier": "^2.8.8",
    "typescript": "^5.1.3"
  },
  "peerDependencies": {
    "n8n-workflow": "*"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
